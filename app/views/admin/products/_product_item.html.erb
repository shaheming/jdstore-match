<tr>
	<td><%= product.id%></td>
	<td id="item-img-td"><%= show_cart_items_image(product)  %></td>
	<td><%= product.title  %></td>
	<td><%= number_to_currency(product.price,:unit=>'$')%></td>
	<td><%= link_to("Edit",edit_admin_product_path(product)) %> |
	<%= link_to("Delete",admin_product_path(product),method: :delete, data: {confirm: "Are you sure?"}) %></td>
</tr>